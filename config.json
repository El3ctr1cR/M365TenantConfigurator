{
    "TenantAdmin": {
        "Username": ""
    },
    "Exchange": {
        "SendFromAlias": true,
        "PlusAddressing": true,
        "MailTips": true,
        "ExternalSenderTags": true,
        "PublicComputerDetection": true,
        "BlockAttachmentDownloadUnmanaged": true,
        "OutlookEventsRecognition": true,
        "ModernAuthentication": true,
        "BlockConsumerStorageOWA": true,
        "BlockOutlookPay": true,
        "RetentionLimitDeletedItems": 30,
        "UnifiedAuditLogSearch": true,
        "AuditLogAgeLimit": 90,
        "DistributionGroupNamingPolicy": "DL_"
    },
    "AzureAD": {
        "BreakGlassAdmin": "MSPNameBG",
        "DeleteStaleDevices": true,
        "SecurityGroups": [
            "Group1",
            "Group2",
            "Group3"
        ]
    },
    "MSOL": {
        "RegionalSettings": {
            "Language": "en-US",
            "TimeZone": "Pacific Standard Time"
        }
    },
    "Teams": {
        "DefaultTeamSettings": {
            "AllowGuestAccess": false,
            "AllowExternalAccess": false
        }
    },
    "SharePointOneDrive": {
        "DefaultStorageLimit": 7000,
        "SharingCapability": "ExternalUserAndGuestSharing",
        "RetentionPolicy": {
            "Email": 7,
            "SharePointOneDrive": 7
        }
    },
    "EnhancedSecurityAlerts": {
        "AdminActivities": true,
        "MalwareAlerts": true,
        "ThreatPolicies": true,
        "HighSensitivity": true,
        "BasicInformational": true
    }
}
